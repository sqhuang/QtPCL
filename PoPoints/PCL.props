<?xml version="1.0" encoding="utf-8"?>
<!--
     PCL 3.0.0属性表文件
     不用单独配置环境变量PCL
     也不用单独在系统PATH上添加PCL可执行路径
     只需要在VisualStudio-"菜单"-"视图"-"属性管理器"中添加现有属性表，添加本文件

     使用前请将下面的PCL_ROOT修改为你的PCL编译输出目录
-->
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <ImportGroup Label="PropertySheets" />
  <PropertyGroup Label="UserMacros" />
  <PropertyGroup>
    <!-- 请把PCL_ROOT属性的值替换为你的PCL编译输出的目标目录，它包含x64、x86这样的子目录 -->
    <PCL_ROOT>D:\dev\PCL 1.8.0</PCL_ROOT>
    <!--VTK配置-->
    <VTK_ROOT>D:\dev\VTK</VTK_ROOT>
    <!-- 指定INCLUDE路径-->
    <IncludePath>$(PCL_ROOT)\include\pcl-1.8;$(PCL_ROOT)\3rdParty\Eigen\eigen3;$(PCL_ROOT)\3rdParty\FLANN\include;$(PCL_ROOT)\3rdParty\OpenNI2\Include;$(PCL_ROOT)\3rdParty\Qhull\include;$(IncludePath)</IncludePath>
    <IncludePath Condition="'$(Configuration)'=='Debug'">$(VTK_ROOT)\VTK-D\include\vtk-7.0;$(IncludePath)</IncludePath>
    <IncludePath Condition="'$(Configuration)'=='Release'">$(VTK_ROOT)\VTK-R\include\vtk-7.0;$(IncludePath)</IncludePath>
    <!-- 指定链接所需lib目录-->
    <LibraryPath>$(PCL_ROOT)\lib;$(PCL_ROOT)\3rdParty\OpenNI2\Lib;$(PCL_ROOT)\3rdParty\FLANN\lib;$(PCL_ROOT)\3rdParty\Qhull\lib;$(LibraryPath)</LibraryPath>
    <!-- 指定VTK链接所需lib目录-->
    <LibraryPath Condition="'$(Configuration)'=='Debug'">$(VTK_ROOT)\VTK-D\lib;$(LibraryPath)</LibraryPath>
    <LibraryPath Condition="'$(Configuration)'=='Release'">$(VTK_ROOT)\VTK-R\lib;$(LibraryPath)</LibraryPath>
    <!-- 保证系统处于Debug模式时，可以使用相对路径读取源代码目录下的文件，比如"lena.jpg"而不必是"d:/work/test/test/lena.jpg" -->
    <LocalDebuggerWorkingDirectory>$(ProjectDir)</LocalDebuggerWorkingDirectory>
    <DebuggerFlavor>WindowsLocalDebugger</DebuggerFlavor>
    <!-- 让系统PATH临时包含指定的dll目录 -->
    <LocalDebuggerEnvironment Condition="'$(Configuration)'=='Debug'">PATH=%PATH%;$(PCL_ROOT)\bin;$(VTK_ROOT)\VTK-D\bin</LocalDebuggerEnvironment>
    <LocalDebuggerEnvironment Condition="'$(Configuration)'=='Release'">PATH=%PATH%;$(PCL_ROOT)\bin;$(VTK_ROOT)\VTK-R\bin</LocalDebuggerEnvironment>
  </PropertyGroup>
